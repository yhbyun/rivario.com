{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/yhbyun/former.git"
        }
    ],
    "require": {
        "laravel/framework": "4.2.*",
        "mccool/laravel-auto-presenter": "2.*",
        "anahkiasen/former": "dev-bootstrapvalidator",
        "edvinaskrucas/notification": "2.*",
        "tijsverkoyen/css-to-inline-styles": "1.3.*@dev",
        "htmlawed/htmlawed": "dev-master",
        "php-ffmpeg/php-ffmpeg": "~0.5"
    },
    "require-dev" : {
        "barryvdh/laravel-debugbar": "1.*",
        "barryvdh/laravel-ide-helper": "1.*",
        "mockery/mockery" : "dev-master",
        "phpunit/phpunit" : "3.7.*",
        "way/generators": "2.*",
        "yhbyun/snowman": "dev-master",
        "squizlabs/php_codesniffer": "2.0.*@dev"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/libraries"
        ],
        "files": [
            "app/libraries/helpers.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
